{
	"name": "frontend_template",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"prepare": "husky install"
