		"css-loader": "^6.8.1",
		"eslint": "^8.40.0",
		"eslint-config-next": "^13.3.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
