		"draftjs-to-html": "^0.9.1",
		"html-to-draftjs": "^1.5.0",
		"immutable": "^4.3.2",
		"next": "13.3.1",
		"next-auth": "^4.22.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-draft-wysiwyg": "^1.15.0",
		"react-hook-form": "^7.45.1",
